<Window x:Class="OblligatorioInterfaces3.opcionesAdministracion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OblligatorioInterfaces3"
        mc:Ignorable="d"
        Title="opcionesAdministracion" Height="450" Width="800">
    <Grid Background="#FFE23B3B">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Button x:Name="veralumno"  Content="VER ALUMNO" HorizontalAlignment="Center" VerticalAlignment="Center" Width="150" Height="150" Background="White" FontSize="22" Grid.Column="0" Click="alumnos"/>
        <Button x:Name="vertemas"  Content="VER TEMAS" HorizontalAlignment="Center" VerticalAlignment="Center" Width="150" Height="150" Background="White" FontSize="22" Grid.Column="1" Click="temas"/>
    </Grid>
</Window>
